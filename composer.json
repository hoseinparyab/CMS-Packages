{
  "name": "theparyab/cms-packages",
  "description": "CMS for practicing writing packages",
  "type": "packages",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "illuminate/support": "^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Theparyab\\CmsPackages\\CmsServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Theparyab\\CmsPackages\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Theparyab\\CmsPackages\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Paryab Hosein",
      "email": "hoseinparyab1@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  }
}

